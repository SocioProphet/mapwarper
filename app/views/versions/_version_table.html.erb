<table class="activity-table">
  <tr>
    <th><%= t('versions.show.time') %></th>
    <th><%= t('versions.show.user') %></th>

    <% if admin_authorized? %>
      <th><%= t('versions.show.ip_address') %></th>
      <th><%= t('versions.show.user_agent') %></th>
    <% end %> 

    <th><%= t('versions.show.object') %></th>
    <th><%= t('versions.show.summary') %></th>
    <th><%= t('versions.show.version') %></th>

    <th></th>
    <% if admin_authorized? %>
      <th></th>
    <% end %>
  </tr>
  <%= render :partial => "version", :collection => @versions unless @versions.nil? %>
</table>
